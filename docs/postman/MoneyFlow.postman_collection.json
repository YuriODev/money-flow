                "key": "Content-Type",
                "value": "multipart/form-data"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions/import/json",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions", "import", "json"]
            },
            "description": "Import subscriptions from JSON file"
          }
        },
        {
          "name": "Import CSV",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "multipart/form-data"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions/import/csv",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions", "import", "csv"]
            },
            "description": "Import subscriptions from CSV file"
          }
        }
      ]
    },
    {
      "name": "AI Agent",
      "item": [
