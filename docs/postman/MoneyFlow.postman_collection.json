            "url": {
              "raw": "{{baseUrl}}/api/v1/agent/execute",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "agent", "execute"]
            },
            "description": "Execute a natural language command"
          }
        }
      ]
    },
