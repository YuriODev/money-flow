{
  "info": {
    "name": "Money Flow API",
    "description": "API collection for Money Flow - Recurring Payment Management",
    "version": "1.0.0",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:8001",
      "type": "string"
    },
    {
      "key": "accessToken",
      "value": "",
      "type": "string"
    },
    {
      "key": "refreshToken",
      "value": "",
      "type": "string"
    }
  ],
  "auth": {
    "type": "bearer",
    "bearer": [
      {
        "key": "token",
        "value": "{{accessToken}}",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Register",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', jsonData.access_token);",
                  "    pm.collectionVariables.set('refreshToken', jsonData.refresh_token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"SecurePassword123!\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/register",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "auth", "register"]
            },
            "description": "Register a new user account"
          }
        },
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', jsonData.access_token);",
                  "    pm.collectionVariables.set('refreshToken', jsonData.refresh_token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"user@example.com\",\n  \"password\": \"SecurePassword123!\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "auth", "login"]
            },
            "description": "Login with existing credentials"
          }
        },
        {
          "name": "Refresh Token",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.collectionVariables.set('accessToken', jsonData.access_token);",
                  "}"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"refresh_token\": \"{{refreshToken}}\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/refresh",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "auth", "refresh"]
            },
            "description": "Get new access token using refresh token"
          }
        },
        {
          "name": "Get Current User",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/me",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "auth", "me"]
            },
            "description": "Get authenticated user profile"
          }
        },
        {
          "name": "Logout",
          "request": {
            "method": "POST",
            "url": {
              "raw": "{{baseUrl}}/api/v1/auth/logout",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "auth", "logout"]
            },
            "description": "Logout and invalidate tokens"
          }
        }
      ]
    },
    {
      "name": "Subscriptions",
      "item": [
        {
          "name": "List Subscriptions",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions"],
              "query": [
                {
                  "key": "is_active",
                  "value": "true",
                  "disabled": true
                },
                {
                  "key": "category",
                  "value": "entertainment",
                  "disabled": true
                },
                {
                  "key": "payment_type",
                  "value": "subscription",
                  "disabled": true
                }
              ]
            },
            "description": "Get all subscriptions with optional filters"
          }
        },
        {
          "name": "Create Subscription",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Netflix\",\n  \"amount\": \"15.99\",\n  \"currency\": \"GBP\",\n  \"frequency\": \"monthly\",\n  \"start_date\": \"2024-01-15\",\n  \"category\": \"entertainment\",\n  \"payment_type\": \"subscription\",\n  \"notes\": \"Family plan\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions"]
            },
            "description": "Create a new subscription"
          }
        },
        {
          "name": "Get Subscription",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions/:id",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "subscription-uuid"
                }
              ]
            },
            "description": "Get a single subscription by ID"
          }
        },
        {
          "name": "Update Subscription",
          "request": {
            "method": "PUT",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"amount\": \"17.99\",\n  \"notes\": \"Updated price\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions/:id",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "subscription-uuid"
                }
              ]
            },
            "description": "Update an existing subscription"
          }
        },
        {
          "name": "Delete Subscription",
          "request": {
            "method": "DELETE",
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions/:id",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions", ":id"],
              "variable": [
                {
                  "key": "id",
                  "value": "subscription-uuid"
                }
              ]
            },
            "description": "Delete a subscription"
          }
        },
        {
          "name": "Get Summary",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions/summary",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions", "summary"]
            },
            "description": "Get spending summary and analytics"
          }
        },
        {
          "name": "Get Upcoming Payments",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions/upcoming?days=30",
              "host": ["{{baseUrl}}"],
              "path": ["api", "v1", "subscriptions", "upcoming"],
              "query": [
                {
                  "key": "days",
                  "value": "30"
                }
              ]
            },
            "description": "Get upcoming payments within specified days"
          }
        }
      ]
    },
    {
      "name": "Import/Export",
      "item": [
        {
          "name": "Export JSON",
          "request": {
            "method": "GET",
            "url": {
              "raw": "{{baseUrl}}/api/v1/subscriptions/export/json",
